Forest_model_area <- gam(DeltaQf_perc ~ DeltaF_perc +
log10(Area_km2) +
Dryness +
Farea_km2 +
Precip_data_type +  Assessment_technique +
Forest_type +
Hydrological_regime
, data = All_data)
summary(Forest_model_area)
write_csv(All_data,"../../data/Data_TotForest_Area.csv")
